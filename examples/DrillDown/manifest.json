{
  "$schema": "https://raw.githubusercontent.com/GeekyEggo/SharpDeck/master/src/SharpDeck.PropertyInspector/manifest-schema.json",
  "Name": "Drill Down Example",
  "Version": "1.0.0",
  "Author": "Richard Herman",
  "Actions": [
    {
      "Name": "Show Numbers",
      "UUID": "com.geekyeggo.drilldown.shownumbers",
      "Icon": "Images/Show/Icon",
      "Tooltip": "Show the numbers",
      "States": [
        {
          "Image": "Images/Show/Image"
        }
      ]
    },
    {
      "Name": "Select Number",
      "UUID": "com.geekyeggo.drilldown.selectnumber",
      "Icon": "Images/Select/Image",
      "Tooltip": "Selects a number",
      "VisibleInActionsList": false,
      "States": [
        {
          "Image": "Images/Select/Image"
        }
      ]
    }
  ],
  "Category": "Drill Down Example",
  "CategoryIcon": "Images/Plugin/CategoryIcon",
  "CodePath": "DrillDown.exe",
  "Description": "An example plug-in that demonstrates the drill down functionality provided by SharpDeck.",
  "Icon": "Images/Plugin/Icon",
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "4.1"
  },
  "OS": [
    {
      "MinimumVersion": "10",
      "Platform": "windows"
    }
  ],
  "Profiles": [
    {
      "Name": "Profiles/DrillDownExampleXL",
      "DeviceType": 2,
      "ReadOnly": true,
      "DontAutoSwitchWhenInstalled": true
    }
  ]
}
