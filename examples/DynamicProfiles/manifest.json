{
    "$schema": "https://raw.githubusercontent.com/GeekyEggo/SharpDeck/master/src/SharpDeck.PropertyInspector/manifest-schema.json",
    "Name": "Dynamic Profiles Demo",
    "Version": "1.0.0",
    "Author": "Richard Herman",
    "Actions": [
        {
            "Name": "Show Numbers",
            "UUID": "com.geekyeggo.dynamicprofiles.shownumbers",
            "Icon": "Images/Show/Icon",
            "Tooltip": "Show the numbers",
            "States": [
                {
                    "Image": "Images/Show/Image"
                }
            ]
        },
        {
            "Name": "Select Number",
            "UUID": "com.geekyeggo.dynamicprofiles.selectnumber",
            "Icon": "Images/Select/Image",
            "Tooltip": "Selects a number",
            "VisibleInActionsList": false,
            "States": [
                {
                    "Image": "Images/Select/Image"
                }
            ]
        }
    ],
    "Category": "Dynamic Profiles Example",
    "CategoryIcon": "Images/Plugin/CategoryIcon",
    "CodePath": "DynamicProfiles.exe",
    "Description": "An example plug-in that demonstrates the dynamic profile functionality provided by SharpDeck.",
    "Icon": "Images/Plugin/Icon",
    "SDKVersion": 2,
    "Software": {
        "MinimumVersion": "4.1"
    },
    "OS": [
        {
            "MinimumVersion": "10",
            "Platform": "windows"
        }
    ],
    "Profiles": [
        {
            "Name": "Profiles/DynamicProfileXL",
            "DeviceType": 2,
            "ReadOnly": true,
            "DontAutoSwitchWhenInstalled": true
        }
    ]
}
